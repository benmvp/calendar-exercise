{
  "name": "calendar-exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eventbrite-react",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-eventbrite-react": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^6.10.3",
    "gh-pages": "^0.12.0",
    "react-scripts": "0.9.5"
  },
  "homepage": "/calendar-exercise",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "yarn run build && gh-pages --dist build --message 'Manual deploy'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
